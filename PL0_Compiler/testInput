/* Should fill entire stack without issue, given stack height of 23 */
var a, b, c, d, e;

procedure x;
	var f, g, h, i;
	procedure y;
		var j, k;
		begin
			j := 10;
			k := i + b /* 11 */
		end;	
	begin
		f := 6;
		g := 7;
		h := 8;
		i := 9;
		call y
	end;

begin
	a := 1;
	b := 2;
	c := 3;
	d := 4;
	e := 5;
	call x
end.